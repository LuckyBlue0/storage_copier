{
  "manifest_version": 3,
  "name": "会话存储复制器",
  "version": "1.0.0",
  "description": "一键复制和导入会话存储数据，支持跨域操作和右键菜单快捷访问",
  "author": "Your Name",
  "default_locale": "zh_CN",
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html"
}
